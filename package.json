{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "manager": "npm run start --prefix manager",
    "client": "npm run start --prefix client",
    "server": "npm run start",
    "server:start": "concurrently \" npm run server \"",
    "client:start": "concurrently \" npm run server \" \"npm run client\" ",
    "manager:start": "concurrently \" npm run server \" \"npm run manager\" ",
    "start:all": "concurrently \" npm run server \" \"npm run client\" \"npm run manager\" ",
    "prepare": "concurrently \"npm install --prefix manager\" \"npm install --prefix client\"  ",
    "project:build": "concurrently \" npm run build \" \"npm run build --prefix manager\" \"npm run build --prefix client\"  ",
    "project:start": "concurrently \"npm run start\"  \"serve -s ./client/build -l tcp://0.0.0.0:3000\"  \"serve -s ./manager/build -l tcp://0.0.0.0:5555\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-telegram-bot-api": "^0.51.4",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "multer": "^1.4.3",
    "node-telegram-bot-api": "^0.54.0",
    "sequelize": "^6.6.5",
    "serve": "^12.0.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.11",
    "@types/sequelize": "^4.28.10",
    "@types/uuid": "^8.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
